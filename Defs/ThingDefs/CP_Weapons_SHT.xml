  <!-- =================== Arasaka KAI ================== -->

  <ThingDef ParentName="BaseHumanGun">
    <defName>Gun_AKAI_CP</defName>
    <label>Arasaka KAI</label>
    <description>10ga shotgun - Arsaka's latest combat shotgun, chambered in 10 gauge, this semi-auto weapon is tube fed.  Excellent for house to house combat.</description>
    <graphicData>
      <texPath>CP/Weapon/AKAI</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>InteractShotgun</soundInteract>
    <statBases>
      <MarketValue>750</MarketValue>
      <AccuracyTouch>0.95</AccuracyTouch>
      <AccuracyShort>0.84</AccuracyShort>
      <AccuracyMedium>0.71</AccuracyMedium>
      <AccuracyLong>0.55</AccuracyLong>
      <RangedWeapon_Cooldown>0.4</RangedWeapon_Cooldown>
    </statBases>

    <destroyOnDrop>false</destroyOnDrop>
    <weaponTags>
      <li>CPGun</li>
      <li>CP_AKAI</li>
    </weaponTags>
    <tradeTags>
      <li>CP</li>
    </tradeTags>

    <equippedStatOffsets>
      <MoveSpeed>-0.10</MoveSpeed>
      <WorkSpeedGlobal>-0.1</WorkSpeedGlobal>
    </equippedStatOffsets>
    <verbs>
      <li Class="Cyberpunk.VerbPropertiesCP">
        <verbClass>Cyberpunk.Verb_ShootCP</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <projectileDef>Bullet_10ga_CP</projectileDef>
